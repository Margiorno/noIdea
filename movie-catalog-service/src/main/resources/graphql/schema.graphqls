type Query {
    movies: [Movie!]!
    movie(id: String!): Movie
}

type Mutation {
    addMovie(movieRequest: MovieRequestDTO!): MovieCreationResponseDTO
}


type Movie @key(fields: "id") {
    id: String!
    title: String
}

input MovieRequestDTO{
    title: String!
}

type MovieCreationResponseDTO{
    success: Boolean!
    message: String!
    id: String
    title: String
}